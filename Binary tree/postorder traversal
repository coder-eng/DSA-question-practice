// 1st approach (recursive)
void postor(Node* root, vector<int> &v){
        if(root==NULL)
        return;
        
       
        postor(root->left,v);
        
        postor(root->right,v);
         v.push_back(root->data);
        
    }
//Function to return a list containing the postorder traversal of the tree.
vector <int> postOrder(Node* root)
{
  // Your code here
   vector<int>v;
  postor(root,v);
        return v;
}

